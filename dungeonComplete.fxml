<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<StackPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fitHeight="400.0" fitWidth="601.0" opacity="0.84">
         <image>
            <Image url="@/mainBackground.jpg" />
         </image>
      </ImageView>
      <Button fx:id="restartButton" mnemonicParsing="false" onAction="#handleRestartButton" text="RETRY" StackPane.alignment="BOTTOM_CENTER">
         <font>
            <Font name="Arial Black" size="13.0" />
         </font>
         <StackPane.margin>
            <Insets bottom="80.0" left="100.0" />
         </StackPane.margin>
         <effect>
            <DropShadow />
         </effect>
      </Button>
      <Text fx:id="statusText" fill="#322a28" strokeType="OUTSIDE" strokeWidth="0.0" text="DUNGEON COMPLETE!" textAlignment="CENTER">
         <font>
            <Font name="Arial Black" size="45.0" />
         </font>
      </Text>
      <Button fx:id="homeButton" mnemonicParsing="false" onAction="#handleHomeButton" text="HOME" StackPane.alignment="BOTTOM_CENTER">
         <font>
            <Font name="Arial Black" size="13.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
         <StackPane.margin>
            <Insets bottom="80.0" right="100.0" />
         </StackPane.margin>
      </Button>
   </children>
</StackPane>
